{
	"name": "anon-messenger",
	"version": "1.0.0",
	"description": "Anonymous terminal messenger over Tor",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"start": "node --max-old-space-size=256 --gc-interval=100 --optimize-for-size index.js",
		"start-fast": "node --max-old-space-size=128 --optimize-for-size --no-warnings index.js",
		"start-secure": "node --max-old-space-size=256 --disable-proto=delete --disallow-code-generation-from-strings index.js"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"keywords": [
		"anonymous",
		"messenger",
		"tor",
		"encryption",
		"privacy",
		"onion"
	],
	"author": "",
	"license": "MIT"
}
